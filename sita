from selenium import webdriver
from selenium.webdriver.common.keys import Keys
from selenium.webdriver.common.by import By
from selenium.webdriver.support.ui import Select
from selenium.webdriver.support.ui import WebDriverWait
from selenium.webdriver.support import expected_conditions as EC

driver = webdriver.Chrome()
driver.get('https://icp.administracionelectronica.gob.es/icpplustieb/citar?p=8&locale=es')

select_province = Select(driver.find_element(By.ID, 'form'))
select_province.select_by_value('08019')

continue_button = driver.find_element(By.NAME, '_eventId_continuar')
continue_button.click()

select_service = Select(WebDriverWait(driver, 10).until(EC.presence_of_element_located((By.ID, 'tramiteGrupo[1]'))))
select_service.select_by_value('595')

select_office = Select(WebDriverWait(driver, 10).until(EC.presence_of_element_located((By.ID, 'oficina'))))
select_office.select_by_index(1)

accept_button = driver.find_element(By.NAME, '_eventId_aceptar')
accept_button.click()

available = driver.find_elements(By.CLASS_NAME, 'available')
if available:
    print('Есть свободные места!')
else:
    print('Свободных мест нет.')

driver.quit()
